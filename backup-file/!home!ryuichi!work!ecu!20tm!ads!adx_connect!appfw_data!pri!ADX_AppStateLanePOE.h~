// ADX_AppStateLanePOE.hで定義されている型をシリアライズした時のバイト数
#define ADX_APPSTATELANEPOE_SERIALIZED_SIZE                                    \
  (1 + 1 + 1 * (4) + 1 + 4 + ADXLOCSMSTATUS_SERIALIZED_SIZE +                  \
   LOCSMREASON_SERIALIZED_SIZE)

// 定数の定義
#define ADX_APP_STATE_LANE_POE_LOC_SM_DRIVE_MODE_REQUEST_MIN ((uint8_t)0)
#define ADX_APP_STATE_LANE_POE_LOC_SM_DRIVE_MODE_REQUEST_MAX ((uint8_t)5)
#define ADX_APP_STATE_LANE_POE_LOC_SM_ROAD_SOULDER_EVACUATION_PROHIBIT_REQUEST_MIN \
  ((uint8_t)0)
#define ADX_APP_STATE_LANE_POE_LOC_SM_ROAD_SOULDER_EVACUATION_PROHIBIT_REQUEST_MAX \
  ((uint8_t)1)
#define ADX_APP_STATE_LANE_POE_LOC_SM_LIMIT_MODE_REQUEST_MIN ((uint8_t)0)
#define ADX_APP_STATE_LANE_POE_LOC_SM_LIMIT_MODE_REQUEST_MAX ((uint8_t)1)
#define ADX_APP_STATE_LANE_POE_LOC_SM_LIMIT_MODE_REQUEST_DIM1 ((size_t)4)
#define ADX_APP_STATE_LANE_POE_LOC_SM_READY_STATUS_MIN ((uint8_t)0)
#define ADX_APP_STATE_LANE_POE_LOC_SM_READY_STATUS_MAX ((uint8_t)2)
#define ADX_APP_STATE_LANE_POE_LOC_SM_TIMESTAMP_MIN ((uint32_t)0)
#define ADX_APP_STATE_LANE_POE_LOC_SM_TIMESTAMP_MAX ((uint32_t)4294967295)

/* ADXアプリ状態 (LanePOE部) */
typedef struct {
  uint8_t loc_sm_drive_mode_request; /* 走行モード要求 */
  uint8_t
      loc_sm_road_soulder_evacuation_prohibit_request; /* 路肩退避禁止要求 */
  uint8_t loc_sm_limit_mode_request[4]; /* 機能制限モード要求 */
  uint8_t loc_sm_ready_status;          /* READY状態 */
  uint32_t loc_sm_timestamp;    /* データ送信タイムスタンプ */
  ADXLocSmStatus loc_sm_status; /* ローカライズ状態 */
  LocSmReason loc_sm_reason;    /* ローカライズ故障不調要因 */
} ADX_AppStateLanePOE;

size_t SerializeADX_AppStateLanePOE(void *buffer, ADX_AppStateLanePOE *data,
                                    size_t max_buffer_size);
size_t SerializeADX_AppStateLanePOEAutoRelease(void *buffer, void *data,
                                               size_t max_buffer_size);
size_t DeserializeADX_AppStateLanePOE(ADX_AppStateLanePOE *data, void *buffer);
bool IsEqualADX_AppStateLanePOE(ADX_AppStateLanePOE *data1,
                                ADX_AppStateLanePOE *data2);
void InitializeADX_AppStateLanePOE(ADX_AppStateLanePOE *data);
void InitializeDefaultOnlyADX_AppStateLanePOE(ADX_AppStateLanePOE *data);

#ifdef __cplusplus
}
#endif

