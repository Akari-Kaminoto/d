#define FUSION_OBJECT_FSN_MULTI_DETECTION_COUNT_MAX ((uint8_t)255)
#define FUSION_OBJECT_RELIABILITY_MIN ((uint64_t)0)
#define FUSION_OBJECT_RELIABILITY_MAX ((uint64_t)1)
#define FUSION_OBJECT_ABS_ACCELERATION_EGOVEHICLE_AXIS_MIN ((float)-10000)
#define FUSION_OBJECT_ABS_ACCELERATION_EGOVEHICLE_AXIS_MAX ((float)10000)
#define FUSION_OBJECT_LONGITUDINAL_ACCELERATION_STATE_MIN ((uint64_t)0)
#define FUSION_OBJECT_LONGITUDINAL_ACCELERATION_STATE_MAX ((uint64_t)3)
#define FUSION_OBJECT_TARGET_THRESHOLD_MIN ((uint8_t)0)
#define FUSION_OBJECT_TARGET_THRESHOLD_MAX ((uint8_t)3)
#define FUSION_OBJECT_WINKER_ON_FLAG_MIN ((uint8_t)0)
#define FUSION_OBJECT_WINKER_ON_FLAG_MAX ((uint8_t)7)
#define FUSION_OBJECT_BRAKE_ON_FLAG_MIN ((uint8_t)0)
#define FUSION_OBJECT_BRAKE_ON_FLAG_MAX ((uint8_t)1)
#define FUSION_OBJECT_LEFT_LANE_WRAP_RATE_AND_IN_LANE_OFFSET_RATE_MIN          \
  ((float)-512)
#define FUSION_OBJECT_LEFT_LANE_WRAP_RATE_AND_IN_LANE_OFFSET_RATE_MAX          \
  ((float)511)
#define FUSION_OBJECT_RIGHT_LANE_WRAP_RATE_MIN ((float)-512)
#define FUSION_OBJECT_RIGHT_LANE_WRAP_RATE_MAX ((float)511)
#define FUSION_OBJECT_ACC_TARGET_OBJECT_FLAG_MIN ((uint8_t)0)
#define FUSION_OBJECT_ACC_TARGET_OBJECT_FLAG_MAX ((uint8_t)1)
#define FUSION_OBJECT_FSN_TRUCK_WIDTH_FLG_MIN ((uint8_t)0)
#define FUSION_OBJECT_FSN_TRUCK_WIDTH_FLG_MAX ((uint8_t)1)

/* 統合物標情報 */
typedef struct {
  uint8_t target_attribute;           /* 物標種別 */
  uint8_t fsn_frd_state;              /* FRD対応付け状態 */
  uint8_t fsn_fsr_r_state;            /* 右側FSR対応付け状態 */
  uint8_t fsn_fsr_l_state;            /* 左側FSR対応付け状態 */
  uint8_t fsn_bsr_r_state;            /* 右側BSR対応付け状態 */
  uint8_t fsn_bsr_l_state;            /* 左側BSR対応付け状態 */
  uint8_t fsn_lidar_state;            /* LiDAR対応付け状態 */
  uint8_t fsn_vcl_jdg_cnt;            /* 車両判定カウンタ */
  uint8_t fsn_fcm_state;              /* 単眼カメラ対応付け状態 */
  uint8_t stop_car_possibility_flg;   /* 静止車両可能性ありフラグ */
  bool is_new;                        /* 新規物標フラグ */
  int32_t count_is_out_of_lane;       /* 路外判定カウンタ */
  uint8_t forward_cutin_target_flag;  /* 前方割り込みフラグ */
  uint8_t forward_cutout_target_flag; /* 前方離脱フラグ */
  uint8_t left_lane_interrupt_flag;   /* 左隣接レーン割込みフラグ */
  uint8_t right_lane_interrupt_flag;  /* 右隣接レーン割込みフラグ */
  uint8_t fsn_fr_lidar_state;         /* 前方LiDAR対応付け状態 */
  uint8_t fsn_rr_lidar_state;         /* 後方LiDAR対応付け状態 */
  uint8_t fsn_lt_lidar_state;         /* 左方LiDAR対応付け状態 */
  uint8_t fsn_rt_lidar_state;         /* 右方LiDAR対応付け状態 */
  uint8_t stationary_object_flag;     /* 静止物フラグ */
  uint8_t moving_object_flag;         /* 移動物フラグ */
  uint8_t oncoming_object_flag;       /* 対向物フラグ */
  int32_t vehicle_count;              /* 車両カウンタ */
  int32_t motorcycle_bicycle_count;   /* 2輪車カウンタ */
  uint8_t extrapolation_object_flag;  /* 外挿フラグ */
  uint8_t over_head_object_flag;      /* 上方構造物フラグ */
  uint8_t
      forward_early_detect_cutin_target_flg; /* 前方割込み（早出し）フラグ */
  bool is_valid;                             /* 有効物標フラグ */
  uint8_t fsn_track_flg;                     /* トラッキングフラグ */
  float rel_height;                          /* 相対高さ */
  uint64_t id;                               /* Fusion物標ID */
  uint64_t age;                              /* 登場回数 */
  uint64_t hidden_age;                       /* 連続外挿フレーム数 */
  Point2D
      uncertain_object_box_center; /* グルーピング探索中心座標(自車後輪軸中心座標系）
                                      */
  Point2D object_box_size; /* 物標長さ、物標幅 */
  Point2D
      object_box_nearest_point; /* 距離（Ｘ）、横位置（Ｙ）(自車後輪軸中心座標系）
                                   */
  float yaw; /* 物標姿勢角（自車後輪軸中心座標系） */
  Velocity2D
      abs_velocity_egovehicle_axis;         /* 距離方向の絶対速度、横位置方向の絶対速度（自車後輪軸中心座標系）
                                               */
  LanePos lane[256];                        /* レーン情報 */
  uint8_t fsn_multi_detection_count;        /* 多重系カウント */
  uint64_t reliability;                     /* 信頼度 */
  float abs_acceleration_egovehicle_axis;   /* 距離方向の絶対加速度 */
  uint64_t longitudinal_acceleration_state; /* 絶対加速度状態 */
  uint8_t target_threshold;                 /* 閾値状態 */
  uint8_t winker_on_flag;                   /* ウィンカ作動状態 */
  uint8_t brake_on_flag;                    /* ブレーキ作動状態 */
  float
      left_lane_wrap_rate_and_in_lane_offset_rate; /* 左白線ラップ率/車線内オフセット率
                                                      */
  float right_lane_wrap_rate;     /* 右白線ラップ率 */
  uint8_t acc_target_object_flag; /* ACC先行車フラグ */
  uint8_t fsn_truck_width_flg;    /* トラック車幅フラグ */
} FusionObject;

size_t SerializeFusionObject(void *buffer, FusionObject *data,
                             size_t max_buffer_size);
size_t DeserializeFusionObject(FusionObject *data, void *buffer);
size_t SerializeFusionObjectAutoRelease(void *buffer, void *data,
                                        size_t max_buffer_size);
size_t DeserializeFusionObjectForDumper(void *data, void *buffer);
bool IsEqualFusionObject(FusionObject *data1, FusionObject *data2);
void InitializeFusionObject(FusionObject *data);
void InitializeDefaultOnlyFusionObject(FusionObject *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_FUSIONOBJECT_H
