#ifndef DATA_DEFINITION_FCMENTITYDATA_ARRAY_H
#define DATA_DEFINITION_FCMENTITYDATA_ARRAY_H

#include "FCMEntityData.h"
#include "appfw.h"
#include "system_constants.h"
#include <stddef.h>

#define FCMENTITYDATA_ARRAY_DATAINFO                                           \
  "{\"name\": \"FCMEntityData_array\", \"description\": "                      \
  "\"FCMEntityData\u306e\u914d\u5217\u3092\u8868\u3059\u69cb\u9020\u4f53\", "  \
  "\"members\": [{\"name\": \"fcm_entities\", "                                \
  "\"type\": " FCMENTITYDATA_DATAINFO                                          \
  ", \"dimension\": [16], \"description\": \"FCM\u7269\u6a19\", "              \
  "\"ignore_naming_rule\": true}], \"ignore_naming_rule\": true, \"ads\": "    \
