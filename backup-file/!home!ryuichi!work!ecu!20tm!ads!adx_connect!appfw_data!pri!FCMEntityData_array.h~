#ifdef __cplusplus
extern "C" {
#endif

// FCMEntityData_array.hで定義されている型をシリアライズした時のバイト数
#define FCMENTITYDATA_ARRAY_SERIALIZED_SIZE                                    \
  (FCMENTITYDATA_SERIALIZED_SIZE * (16))

// 定数の定義
#define FCM_ENTITY_DATA_ARRAY_FCM_ENTITIES_DIM1 ((size_t)16)

/* FCMEntityDataの配列を表す構造体 */
typedef struct {
  FCMEntityData fcm_entities[16]; /* FCM物標 */
} FCMEntityData_array;

size_t SerializeFCMEntityData_array(void *buffer, FCMEntityData_array *data,
                                    size_t max_buffer_size);
size_t SerializeFCMEntityData_arrayAutoRelease(void *buffer, void *data,
                                               size_t max_buffer_size);
size_t DeserializeFCMEntityData_array(FCMEntityData_array *data, void *buffer);
bool IsEqualFCMEntityData_array(FCMEntityData_array *data1,
                                FCMEntityData_array *data2);
void InitializeFCMEntityData_array(FCMEntityData_array *data);
void InitializeDefaultOnlyFCMEntityData_array(FCMEntityData_array *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_FCMENTITYDATA_ARRAY_H
