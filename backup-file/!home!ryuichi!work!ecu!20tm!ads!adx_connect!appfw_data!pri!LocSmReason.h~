#ifdef __cplusplus
extern "C" {
#endif

// LocSmReason.hで定義されている型をシリアライズした時のバイト数
#define LOCSMREASON_SERIALIZED_SIZE                                            \
  (LANEIDSTABILITYDATA_SERIALIZED_SIZE + FCMSTATUSDATA_SERIALIZED_SIZE +       \
   LOCCAMERAADXSTATUS_SERIALIZED_SIZE + LCMSTATUS_SERIALIZED_SIZE +            \
   PVMSTATUSDATA_SERIALIZED_SIZE + SISSTATUSDATA_SERIALIZED_SIZE +             \
   ADXECUSTATUS_SERIALIZED_SIZE + INNERCAMERASTATUS_SERIALIZED_SIZE +          \
   TELECAMERASTATUS_SERIALIZED_SIZE + LOCSOCSTATUS_SERIALIZED_SIZE +           \
   LOCSTATUS_SERIALIZED_SIZE)

// 定数の定義

/* ローカライズ故障不調要因 */
typedef struct {
  LaneIdStabilityData lane_id_status; /* LaneID不調要因 */
  FcmStatusData fcm_status;           /* FCM故障不調要因 */
  LocCameraADXStatus lcm_adx_status;  /* ロケカメ(ADX)故障不調要因 */
  LcmStatus lcm_status;               /* ロケカメ故障不調要因 */
  PvmStatusData pvm_status;           /* PVM故障不調要因 */
  SisStatusData sis_status;           /* SIS故障不調要因 */
  AdxEcuStatus adxecu_status;         /* ADX-ECU故障不調要因 */
  InnerCameraStatus inner_camera_status; /* 電子インナカメラ故障不調要因 */
  TeleCameraStatus tele_camera_status; /* 望遠カメラ故障不調要因 */
  locSocStatus loc_soc_status; /* ローカライズSoC演算異常要因 */
  LocStatus loc_status;        /* ローカライズ性能限界 */
} LocSmReason;

size_t SerializeLocSmReason(void *buffer, LocSmReason *data,
                            size_t max_buffer_size);
size_t DeserializeLocSmReason(LocSmReason *data, void *buffer);
size_t SerializeLocSmReasonAutoRelease(void *buffer, void *data,
                                       size_t max_buffer_size);
size_t DeserializeLocSmReasonForDumper(void *data, void *buffer);
bool IsEqualLocSmReason(LocSmReason *data1, LocSmReason *data2);
void InitializeLocSmReason(LocSmReason *data);
void InitializeDefaultOnlyLocSmReason(LocSmReason *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_LOCSMREASON_H
