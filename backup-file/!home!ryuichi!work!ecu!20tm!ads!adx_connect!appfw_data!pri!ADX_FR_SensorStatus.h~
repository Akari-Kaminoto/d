#ifdef __cplusplus
extern "C" {
#endif

// ADX_FR_SensorStatus.hで定義されている型をシリアライズした時のバイト数
#define ADX_FR_SENSORSTATUS_SERIALIZED_SIZE                                    \
  (CARSTATE_SERIALIZED_SIZE + ADX_FAILURESTATUS_SERIALIZED_SIZE +              \
   ADX_OBJECTRECOGNITIONFAILURE_SERIALIZED_SIZE +                              \
   ADX_AXISGAP_SERIALIZED_SIZE + ADX_AXISGAP_SERIALIZED_SIZE +                 \
   ADX_WASHINGREQUEST_SERIALIZED_SIZE)

// 定数の定義

/* 前後センサ状態情報 */
typedef struct {
  CarState car_state;                          /* 自車位置情報 */
  ADX_FailureStatus failure_status;            /* 不調状態 */
  ADX_ObjectRecognitionFailure object_failure; /* 物標認識不調要因 */
  ADX_AxisGap horizon_axis_gap;                /* 水平軸ズレ情報 */
  ADX_AxisGap vertical_axis_gap;               /* 垂直軸ズレ情報 */
  ADX_WashingRequest washing_request;          /* 洗浄要求 */
} ADX_FR_SensorStatus;

size_t SerializeADX_FR_SensorStatus(void *buffer, ADX_FR_SensorStatus *data,
                                    size_t max_buffer_size);
size_t DeserializeADX_FR_SensorStatus(ADX_FR_SensorStatus *data, void *buffer);
size_t SerializeADX_FR_SensorStatusAutoRelease(void *buffer, void *data,
                                               size_t max_buffer_size);
size_t DeserializeADX_FR_SensorStatusForDumper(void *data, void *buffer);
bool IsEqualADX_FR_SensorStatus(ADX_FR_SensorStatus *data1,
                                ADX_FR_SensorStatus *data2);
void InitializeADX_FR_SensorStatus(ADX_FR_SensorStatus *data);
void InitializeDefaultOnlyADX_FR_SensorStatus(ADX_FR_SensorStatus *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_ADX_FR_SENSORSTATUS_H
