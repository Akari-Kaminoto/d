#ifdef __cplusplus
extern "C" {
#endif

// ADX_LcmState.hで定義されている型をシリアライズした時のバイト数
#define ADX_LCMSTATE_SERIALIZED_SIZE                                           \
  (CARSTATE_SERIALIZED_SIZE + 4 + 4 +                                          \
   ADX_OBJECTRECOGNITIONFAILURE_SERIALIZED_SIZE +                              \
   ADX_LANERECOGNITIONUNSTABLE_SERIALIZED_SIZE)

// 定数の定義
#define ADX_LCM_STATE_ADX_LCM_FAILURE_FLAG_MIN ((uint32_t)0)
#define ADX_LCM_STATE_ADX_LCM_FAILURE_FLAG_MAX ((uint32_t)4294967295)
#define ADX_LCM_STATE_ADX_LCM_UNSTABLE_FLAG_MIN ((uint32_t)0)
#define ADX_LCM_STATE_ADX_LCM_UNSTABLE_FLAG_MAX ((uint32_t)4294967295)

/* カメラ状態情報 (ロケカメ) */
typedef struct {
  CarState adx_lcm_car_state;     /* 自車位置情報 */
  uint32_t adx_lcm_failure_flag;  /* ADXロケカメ故障フラグ */
  uint32_t adx_lcm_unstable_flag; /* ADXロケカメ不調フラグ */
  ADX_ObjectRecognitionFailure
      adx_lcm_object_failure; /* ADXロケカメ物標認識不調要因 */
  ADX_LaneRecognitionUnstable
      adx_lcm_lane_unstable; /* ADXロケカメレーン認識不調要因 */
} ADX_LcmState;

size_t SerializeADX_LcmState(void *buffer, ADX_LcmState *data,
                             size_t max_buffer_size);
size_t SerializeADX_LcmStateAutoRelease(void *buffer, void *data,
                                        size_t max_buffer_size);
size_t DeserializeADX_LcmState(ADX_LcmState *data, void *buffer);
bool IsEqualADX_LcmState(ADX_LcmState *data1, ADX_LcmState *data2);
void InitializeADX_LcmState(ADX_LcmState *data);
void InitializeDefaultOnlyADX_LcmState(ADX_LcmState *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_ADX_LCMSTATE_H
