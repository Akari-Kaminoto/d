#ifndef DATA_DEFINITION_ADXECUSTATUS_H
#define DATA_DEFINITION_ADXECUSTATUS_H

#include "AdxEcuStatusFailure.h"
#include "appfw.h"
#include "system_constants.h"
#include <stddef.h>

#define ADXECUSTATUS_DATAINFO                                                  \
  "{\"name\": \"AdxEcuStatus\", \"description\": "                             \
  "\"ADX-ECU\u6545\u969c\u4e0d\u8abf\u8981\u56e0\", \"members\": [{\"name\": " \
  "\"adxecu_status_failure\", \"type\": " ADXECUSTATUSFAILURE_DATAINFO         \
  ", \"description\": \"ADX-ECU\u6545\u969c\u8981\u56e0\", "                   \
  "\"ignore_naming_rule\": true}], \"ignore_naming_rule\": true, \"ads\": "    \
  "false, \"name_snake_case\": \"adx_ecu_status\"}"

#ifdef __cplusplus
extern "C" {
#endif

// AdxEcuStatus.hで定義されている型をシリアライズした時のバイト数
#define ADXECUSTATUS_SERIALIZED_SIZE (ADXECUSTATUSFAILURE_SERIALIZED_SIZE)

// 定数の定義

/* ADX-ECU故障不調要因 */
typedef struct {
  AdxEcuStatusFailure adxecu_status_failure; /* ADX-ECU故障要因 */
} AdxEcuStatus;

size_t SerializeAdxEcuStatus(void *buffer, AdxEcuStatus *data,
                             size_t max_buffer_size);
size_t SerializeAdxEcuStatusAutoRelease(void *buffer, void *data,
                                        size_t max_buffer_size);
size_t DeserializeAdxEcuStatus(AdxEcuStatus *data, void *buffer);
bool IsEqualAdxEcuStatus(AdxEcuStatus *data1, AdxEcuStatus *data2);
void InitializeAdxEcuStatus(AdxEcuStatus *data);
void InitializeDefaultOnlyAdxEcuStatus(AdxEcuStatus *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_ADXECUSTATUS_H
