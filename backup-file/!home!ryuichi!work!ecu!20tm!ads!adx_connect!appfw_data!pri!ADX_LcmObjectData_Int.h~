// ADX_LcmObjectData_Int.hで定義されている型をシリアライズした時のバイト数
#define ADX_LCMOBJECTDATA_INT_SERIALIZED_SIZE                                  \
  (CARSTATE_SERIALIZED_SIZE + OBJINFO_INT_SERIALIZED_SIZE * (256) + 1 + 1)

// 定数の定義
#define ADX_LCM_OBJECT_DATA_INT_ADX_LCM_OBJ_STATUS_DIM1 ((size_t)256)
#define ADX_LCM_OBJECT_DATA_INT_ADX_LCM_CNT_MAP_NUM_MIN ((uint8_t)0)
#define ADX_LCM_OBJECT_DATA_INT_ADX_LCM_CNT_MAP_NUM_MAX ((uint8_t)255)
#define ADX_LCM_OBJECT_DATA_INT_ADX_LCM_EXP_CNT_FLAG_MIN ((uint8_t)0)
#define ADX_LCM_OBJECT_DATA_INT_ADX_LCM_EXP_CNT_FLAG_MAX ((uint8_t)255)

/* 物標情報 (ロケカメ) */
typedef struct {
  CarState adx_obj_lcm_car_state;      /* 自車位置情報 */
  ObjInfo_Int adx_lcm_obj_status[256]; /* 物標情報 */
  uint8_t adx_lcm_cnt_map_num;  /* マニュアル制御マップナンバー */
  uint8_t adx_lcm_exp_cnt_flag; /* 露出マニュアル制御フラグ */
} ADX_LcmObjectData_Int;

size_t SerializeADX_LcmObjectData_Int(void *buffer, ADX_LcmObjectData_Int *data,
                                      size_t max_buffer_size);
size_t SerializeADX_LcmObjectData_IntAutoRelease(void *buffer, void *data,
                                                 size_t max_buffer_size);
size_t DeserializeADX_LcmObjectData_Int(ADX_LcmObjectData_Int *data,
                                        void *buffer);
bool IsEqualADX_LcmObjectData_Int(ADX_LcmObjectData_Int *data1,
                                  ADX_LcmObjectData_Int *data2);
void InitializeADX_LcmObjectData_Int(ADX_LcmObjectData_Int *data);
void InitializeDefaultOnlyADX_LcmObjectData_Int(ADX_LcmObjectData_Int *data);

#ifdef __cplusplus
}
