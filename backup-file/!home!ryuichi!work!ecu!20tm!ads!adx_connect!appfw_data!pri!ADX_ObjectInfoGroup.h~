#ifndef DATA_DEFINITION_ADX_OBJECTINFOGROUP_H
#define DATA_DEFINITION_ADX_OBJECTINFOGROUP_H

#include "ADX_BasicInfoGroup.h"
#include "ADX_IdentificationInfoGroup.h"
#include "ADX_RectangularInfoGroup.h"
#include "ADX_SpeedInfoGroup.h"
#include "appfw.h"
#include "system_constants.h"
#include <stddef.h>

#define ADX_OBJECTINFOGROUP_DATAINFO                                           \
  "{\"name\": \"ADX_ObjectInfoGroup\", \"description\": "                      \
  "\"\u7269\u6a19\u60c5\u5831\u7fa4\", \"members\": [{\"name\": "              \
  "\"basic_info_group\", \"type\": " ADX_BASICINFOGROUP_DATAINFO               \
  ", \"description\": \"\u57fa\u672c\u60c5\u5831\u7fa4\", "                    \
  "\"ignore_naming_rule\": true}, {\"name\": \"rectangular_info_group\", "     \
  "\"type\": " ADX_RECTANGULARINFOGROUP_DATAINFO                               \
