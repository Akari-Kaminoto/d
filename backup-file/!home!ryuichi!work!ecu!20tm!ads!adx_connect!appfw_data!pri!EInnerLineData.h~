// EInnerLineData.hで定義されている型をシリアライズした時のバイト数
#define EINNERLINEDATA_SERIALIZED_SIZE                                         \
  (1 + 2 + 2 + LINEDATAE_SERIALIZED_SIZE * (960) + 2 +                         \
   LINEDATAE_SERIALIZED_SIZE * (960) + 2 + 2 + 2 + 4 + 2 + 2 + 2 + 2 + 2 + 1 + \
   2 + 2 + 1 + 2 + CARSTATE_SERIALIZED_SIZE)

// 定数の定義
#define E_INNER_LINE_DATA_AVAILABLE_FLAG_MIN ((bool)0)
#define E_INNER_LINE_DATA_AVAILABLE_FLAG_MAX ((bool)1)
#define E_INNER_LINE_DATA_CAM_HEIGHT_MIN ((uint16_t)0)
#define E_INNER_LINE_DATA_CAM_HEIGHT_MAX ((uint16_t)65535)
#define E_INNER_LINE_DATA_CAM_TIRE_DIST_MIN ((int16_t)-32768)
#define E_INNER_LINE_DATA_CAM_TIRE_DIST_MAX ((int16_t)32767)
#define E_INNER_LINE_DATA_LEFT_LINE_DATA_DIM1 ((size_t)960)
#define E_INNER_LINE_DATA_CAM_LATERAL_DIST_MIN ((int16_t)-32768)
#define E_INNER_LINE_DATA_CAM_LATERAL_DIST_MAX ((int16_t)32767)
#define E_INNER_LINE_DATA_RIGHT_LINE_DATA_DIM1 ((size_t)960)
#define E_INNER_LINE_DATA_CAM_IMAGE_WIDTH_MIN ((uint16_t)0)
#define E_INNER_LINE_DATA_CAM_IMAGE_WIDTH_MAX ((uint16_t)65535)
#define E_INNER_LINE_DATA_CAM_IMAGE_HEIGHT_MIN ((uint16_t)0)
#define E_INNER_LINE_DATA_CAM_IMAGE_HEIGHT_MAX ((uint16_t)65535)
#define E_INNER_LINE_DATA_OFFSET_MIN ((int16_t)-32768)
#define E_INNER_LINE_DATA_OFFSET_MAX ((int16_t)32767)
#define E_INNER_LINE_DATA_FOCAL_LENGTH_MIN ((uint32_t)0)
#define E_INNER_LINE_DATA_FOCAL_LENGTH_MAX ((uint32_t)4294967295)
#define E_INNER_LINE_DATA_YAW_MIN ((int16_t)-32768)
#define E_INNER_LINE_DATA_YAW_MAX ((int16_t)32767)
#define E_INNER_LINE_DATA_STATIC_FOE_X_MIN ((uint16_t)0)
#define E_INNER_LINE_DATA_STATIC_FOE_X_MAX ((uint16_t)65535)
#define E_INNER_LINE_DATA_CURVATURE_MIN ((int16_t)-32768)
#define E_INNER_LINE_DATA_CURVATURE_MAX ((int16_t)32767)
#define E_INNER_LINE_DATA_STATIC_FOE_Y_MIN ((uint16_t)0)
#define E_INNER_LINE_DATA_STATIC_FOE_Y_MAX ((uint16_t)65535)
#define E_INNER_LINE_DATA_CURVATURE_CHANGE_RATE_MIN ((int16_t)-32768)
#define E_INNER_LINE_DATA_CURVATURE_CHANGE_RATE_MAX ((int16_t)32767)
#define E_INNER_LINE_DATA_STATIC_FOE_STATE_MIN ((uint8_t)0)
#define E_INNER_LINE_DATA_STATIC_FOE_STATE_MAX ((uint8_t)2)
#define E_INNER_LINE_DATA_DYNAMIC_FOE_X_MIN ((uint16_t)0)
#define E_INNER_LINE_DATA_DYNAMIC_FOE_X_MAX ((uint16_t)65535)
#define E_INNER_LINE_DATA_DYNAMIC_FOE_Y_MIN ((uint16_t)0)
#define E_INNER_LINE_DATA_DYNAMIC_FOE_Y_MAX ((uint16_t)65535)
#define E_INNER_LINE_DATA_DYNAMIC_FOE_FLAG_MIN ((bool)0)
#define E_INNER_LINE_DATA_DYNAMIC_FOE_FLAG_MAX ((bool)1)
#define E_INNER_LINE_DATA_INIT_ROLL_MIN ((int16_t)-32768)
#define E_INNER_LINE_DATA_INIT_ROLL_MAX ((int16_t)32767)

/* 後方区画線情報 */
typedef struct {
  bool available_flag;            /* 利用可否フラグ */
  uint16_t cam_height;            /* カメラ高さ */
  int16_t cam_tire_dist;          /* カメラ前輪間距離 */
  LineDataE left_line_data[960];  /* 区画線候補点（左） */
  int16_t cam_lateral_dist;       /* カメラ横位置 */
  LineDataE right_line_data[960]; /* 区画線候補点（右） */
  uint16_t cam_image_width;       /* カメラ画像幅 */
  uint16_t cam_image_height;      /* カメラ画像高さ */
  int16_t offset;                 /* オフセット */
  uint32_t focal_length;          /* 焦点距離 */
  int16_t yaw;                    /* ヨー角 */
  uint16_t static_foe_x;          /* 静的FOE x座標 */
  int16_t curvature;              /* 曲率 */
  uint16_t static_foe_y;          /* 静的FOE y座標 */
  int16_t curvature_change_rate;  /* 曲率変化率 */
  uint8_t static_foe_state;       /* 静的FOE学習状態 */
  uint16_t dynamic_foe_x;         /* 動的FOE x座標 */
  uint16_t dynamic_foe_y;         /* 動的FOE y座標 */
  bool dynamic_foe_flag;          /* 動的FOE算出フラグ */
  int16_t init_roll;              /* 初期ロール角 */
  CarState car_pose_loc;          /* 自車情報 */
} EInnerLineData;

size_t SerializeEInnerLineData(void *buffer, EInnerLineData *data,
                               size_t max_buffer_size);
size_t DeserializeEInnerLineData(EInnerLineData *data, void *buffer);
size_t SerializeEInnerLineDataAutoRelease(void *buffer, void *data,
                                          size_t max_buffer_size);
size_t DeserializeEInnerLineDataForDumper(void *data, void *buffer);
bool IsEqualEInnerLineData(EInnerLineData *data1, EInnerLineData *data2);
void InitializeEInnerLineData(EInnerLineData *data);
void InitializeDefaultOnlyEInnerLineData(EInnerLineData *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_EINNERLINEDATA_H
