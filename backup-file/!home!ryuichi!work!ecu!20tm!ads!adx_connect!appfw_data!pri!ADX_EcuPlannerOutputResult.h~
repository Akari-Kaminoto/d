#ifdef __cplusplus
extern "C" {
#endif

// ADX_EcuPlannerOutputResult.hで定義されている型をシリアライズした時のバイト数
#define ADX_ECUPLANNEROUTPUTRESULT_SERIALIZED_SIZE                             \
  (CARSTATE_SERIALIZED_SIZE + USDCOMMONINFO_SERIALIZED_SIZE +                  \
   LANESTATEPREDICTION_SERIALIZED_SIZE + OBJECTPREDICTION_SERIALIZED_SIZE +    \
   MRGSPDRESULT_SERIALIZED_SIZE + MRGSPCRESULT_SERIALIZED_SIZE)

// 定数の定義

/* ADX-ECUのプランナ出力結果 */
typedef struct {
  CarState car_state;                        /* 自車位置情報 */
  UsdCommonInfo usd_common_info;             /* タイムスタンプなど */
  LaneStatePrediction lane_state_prediction; /* レーン状態予測結果 */
  ObjectPrediction object_prediction;        /* 物標行動予測結果 */
  MrgSpdResult mrd_spd_result; /* 合流進入車速推奨判断結果 */
  MrgSpcResult mrd_spc_result; /* 合流先判断結果 */
} ADX_EcuPlannerOutputResult;

size_t SerializeADX_EcuPlannerOutputResult(void *buffer,
                                           ADX_EcuPlannerOutputResult *data,
                                           size_t max_buffer_size);
size_t SerializeADX_EcuPlannerOutputResultAutoRelease(void *buffer, void *data,
                                                      size_t max_buffer_size);
size_t DeserializeADX_EcuPlannerOutputResult(ADX_EcuPlannerOutputResult *data,
                                             void *buffer);
bool IsEqualADX_EcuPlannerOutputResult(ADX_EcuPlannerOutputResult *data1,
                                       ADX_EcuPlannerOutputResult *data2);
void InitializeADX_EcuPlannerOutputResult(ADX_EcuPlannerOutputResult *data);
void InitializeDefaultOnlyADX_EcuPlannerOutputResult(
    ADX_EcuPlannerOutputResult *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_ADX_ECUPLANNEROUTPUTRESULT_H
