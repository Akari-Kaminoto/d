#ifndef DATA_DEFINITION_FUSIONOBJECT_ARRAY_H
#define DATA_DEFINITION_FUSIONOBJECT_ARRAY_H

#include "FusionObject.h"
#include "appfw.h"
#include "system_constants.h"
#include <stddef.h>

#define FUSIONOBJECT_ARRAY_DATAINFO                                            \
  "{\"name\": \"FusionObject_array\", \"description\": "                       \
  "\"\u7d71\u5408\u7269\u6a19\u60c5\u5831\u306e\u914d\u5217\u3092\u8868\u3059" \
  "\u69cb\u9020\u4f53\", \"members\": [{\"name\": \"fsn_objects\", "           \
  "\"type\": " FUSIONOBJECT_DATAINFO                                           \
  ", \"dimension\": [166], \"description\": "                                  \
  "\"\u7d71\u5408\u7269\u6a19\u60c5\u5831\", \"ignore_naming_rule\": true}], " \
  "\"ignore_naming_rule\": true, \"ads\": false, \"name_snake_case\": "        \
  "\"fusion_object_array\"}"

#ifdef __cplusplus
extern "C" {
#endif

// FusionObject_array.hで定義されている型をシリアライズした時のバイト数
#define FUSIONOBJECT_ARRAY_SERIALIZED_SIZE                                     \
  (FUSIONOBJECT_SERIALIZED_SIZE * (166))

// 定数の定義
#define FUSION_OBJECT_ARRAY_FSN_OBJECTS_DIM1 ((size_t)166)

/* 統合物標情報の配列を表す構造体 */
typedef struct {
  FusionObject fsn_objects[166]; /* 統合物標情報 */
} FusionObject_array;

size_t SerializeFusionObject_array(void *buffer, FusionObject_array *data,
                                   size_t max_buffer_size);
size_t SerializeFusionObject_arrayAutoRelease(void *buffer, void *data,
                                              size_t max_buffer_size);
size_t DeserializeFusionObject_array(FusionObject_array *data, void *buffer);
bool IsEqualFusionObject_array(FusionObject_array *data1,
                               FusionObject_array *data2);
void InitializeFusionObject_array(FusionObject_array *data);
void InitializeDefaultOnlyFusionObject_array(FusionObject_array *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_FUSIONOBJECT_ARRAY_H
