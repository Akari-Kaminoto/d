// ADX_TpcmObjectData_Int.hで定義されている型をシリアライズした時のバイト数
#define ADX_TPCMOBJECTDATA_INT_SERIALIZED_SIZE                                 \
  (CARSTATE_SERIALIZED_SIZE + OBJINFO_INT_SERIALIZED_SIZE * (256))

// 定数の定義
#define ADX_TPCM_OBJECT_DATA_INT_ADX_TPCM_OBJ_STATUS_DIM1 ((size_t)256)

/* 物標情報 (望遠) */
typedef struct {
  CarState adx_obj_tpcm_car_state;      /* 自車位置情報 */
  ObjInfo_Int adx_tpcm_obj_status[256]; /* 物標情報 */
} ADX_TpcmObjectData_Int;

