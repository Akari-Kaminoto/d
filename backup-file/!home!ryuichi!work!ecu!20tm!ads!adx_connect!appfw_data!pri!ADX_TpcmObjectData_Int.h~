                                   ADX_TpcmObjectData_Int *data2);
void InitializeADX_TpcmObjectData_Int(ADX_TpcmObjectData_Int *data);
void InitializeDefaultOnlyADX_TpcmObjectData_Int(ADX_TpcmObjectData_Int *data);

