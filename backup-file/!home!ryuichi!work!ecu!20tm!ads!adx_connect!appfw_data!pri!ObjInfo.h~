/* 物標情報 */
typedef struct {
  uint8_t attribute;                  /* 物標種別 */
  uint8_t attribute_likelihood;       /* 物標尤度 */
  float dist;                         /* 距離（縦方向） */
  uint8_t dist_reliability;           /* 距離（縦方向）信頼度 */
  float lateral_loc;                  /* 横位置 */
  uint8_t lateral_reliability;        /* 横位置 信頼度 */
  float relative_speed;               /* 相対縦速度 */
  float relative_lateral_velocity;    /* 相対横速度 */
  float absolute_speed;               /* 絶対縦速度 */
  uint8_t absolute_speed_reliability; /* 絶対縦速度 信頼度 */
  float absolute_lateral_velocity;    /* 絶対横速度 */
  uint8_t absolute_lateral_velocity_reliability; /* 絶対横速度 信頼度 */
  float width;                                   /* 横幅 */
  float height;                                  /* 高さ幅 */
  float horizontal_angle;                        /* 方位角度 */
  float vertical_angle;                          /* 高さ角度 */
  float heading;                                 /* 向き */
  uint8_t heading_reliability;                   /* 向き 信頼度 */
  float depth;                                   /* 奥行き */
  float angle_width;                             /* 角度横幅 */
  float angle_height;                            /* 角度高さ幅 */
  float camera_position_ttc;                     /* カメラ位置TTC */
  uint8_t ttc_confidence;                        /* TTC信頼度 */
  float expansion_rate;                          /* 拡大率 */
  uint8_t fsn_status;                            /* FSN状態 */
  float fsn_dist;                                /* FSN距離（縦方向） */
  float fsn_lateral_loc;                         /* FSN横位置 */
  float fsn_relative_speed;                      /* FSN相対縦速度 */
  float fsn_relative_lateral_velocity;           /* FSN相対横速度 */
  float fsn_absolute_speed;                      /* FSN絶対縦速度 */
  float fsn_absolute_lateral_velocity;           /* FSN絶対横速度 */
  float fsn_width;                               /* FSN横幅 */
  float fsn_height;                              /* FSN高さ幅 */
  float fsn_heading;                             /* FSN向き */
  float fsn_depth;                               /* FSN奥行き */
  uint8_t new_object_flag;                       /* 新規物標フラグ */
  uint16_t tracking_count;             /* 物標信頼度1（観測回数) */
  uint8_t hidden_count;                /* 物標信頼度2 (ロスト回数) */
  uint8_t extrapolating_flag;          /* 物標信頼度3 (外挿状態) */
  ObjCarInfo obj_car_info;             /* 車両認識関連情報 */
  ObjWalkerInfo obj_walker_info;       /* 歩行者認識情報 */
  ObjBicycleInfo obj_bicycle_info;     /* 自転車認識情報 */
  ObjSignboardInfo obj_signboard_info; /* 看板認識情報 */
  ObjFallingInfo obj_falling_info;     /* 落下物認識情報 */
  ObjConstructionEquipmentInfo
      obj_construction_equipment_info; /* 工事現場認識情報 */
  ObjMiscInfo obj_misc_info;           /* その他物標認識情報 */
  ExtensionArea extension_area;        /* 拡張領域 */
} ObjInfo;

size_t SerializeObjInfo(void *buffer, ObjInfo *data, size_t max_buffer_size);
size_t DeserializeObjInfo(ObjInfo *data, void *buffer);
size_t SerializeObjInfoAutoRelease(void *buffer, void *data,
                                   size_t max_buffer_size);
size_t DeserializeObjInfoForDumper(void *data, void *buffer);
bool IsEqualObjInfo(ObjInfo *data1, ObjInfo *data2);
void InitializeObjInfo(ObjInfo *data);
void InitializeDefaultOnlyObjInfo(ObjInfo *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_OBJINFO_H
