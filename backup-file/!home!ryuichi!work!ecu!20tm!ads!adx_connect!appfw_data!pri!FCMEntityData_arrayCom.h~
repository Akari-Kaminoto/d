// FCMEntityData_arrayCom.hで定義されている型をシリアライズした時のバイト数
#define FCMENTITYDATA_ARRAYCOM_SERIALIZED_SIZE                                 \
  (FCMENTITYDATA_SERIALIZED_SIZE * (16))

// 定数の定義
#define FCM_ENTITY_DATA_ARRAY_COM_FCM_ENTITIES_DIM1 ((size_t)16)

/* FCMEntityDataの配列を表す構造体 */
typedef struct {
  FCMEntityData fcm_entities[16]; /* FCM物標 */
} FCMEntityData_arrayCom;

