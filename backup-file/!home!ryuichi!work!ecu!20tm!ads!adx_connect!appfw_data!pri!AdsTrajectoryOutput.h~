#ifndef DATA_DEFINITION_ADSTRAJECTORYOUTPUT_H
#define DATA_DEFINITION_ADSTRAJECTORYOUTPUT_H

#include "TargetTrajectory.h"
#include "appfw.h"
#include "system_constants.h"
#include <stddef.h>

#define ADSTRAJECTORYOUTPUT_DATAINFO                                           \
  "{\"name\": \"AdsTrajectoryOutput\", \"description\": "                      \
  "\"ADS\u30c8\u30e9\u30b8\u30a7\u30af\u30c8\u30ea\u51fa\u529b\", "            \
  "\"members\": [{\"name\": \"target_trajectory\", "                           \
  "\"type\": " TARGETTRAJECTORY_DATAINFO                                       \
  ", \"description\": "                                                        \
  "\"\u76ee\u6a19\u30c8\u30e9\u30b8\u30a7\u30af\u30c8\u30ea\", "               \
  "\"ignore_naming_rule\": true}], \"ignore_naming_rule\": true, \"ads\": "    \
  "false, \"name_snake_case\": \"ads_trajectory_output\"}"

#ifdef __cplusplus
extern "C" {
#endif

// AdsTrajectoryOutput.hで定義されている型をシリアライズした時のバイト数
#define ADSTRAJECTORYOUTPUT_SERIALIZED_SIZE (TARGETTRAJECTORY_SERIALIZED_SIZE)

// 定数の定義

/* ADSトラジェクトリ出力 */
typedef struct {
  TargetTrajectory target_trajectory; /* 目標トラジェクトリ */
} AdsTrajectoryOutput;

size_t SerializeAdsTrajectoryOutput(void *buffer, AdsTrajectoryOutput *data,
                                    size_t max_buffer_size);
size_t SerializeAdsTrajectoryOutputAutoRelease(void *buffer, void *data,
                                               size_t max_buffer_size);
size_t DeserializeAdsTrajectoryOutput(AdsTrajectoryOutput *data, void *buffer);
bool IsEqualAdsTrajectoryOutput(AdsTrajectoryOutput *data1,
                                AdsTrajectoryOutput *data2);
void InitializeAdsTrajectoryOutput(AdsTrajectoryOutput *data);
void InitializeDefaultOnlyAdsTrajectoryOutput(AdsTrajectoryOutput *data);

#ifdef __cplusplus
}
#endif

#endif // DATA_DEFINITION_ADSTRAJECTORYOUTPUT_H
