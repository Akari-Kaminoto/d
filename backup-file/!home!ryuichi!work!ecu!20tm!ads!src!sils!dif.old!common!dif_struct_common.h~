	U8								u8_starting_lane_block_id;				/* 空間情報切り出し時の起点レーンブロックID */
	U4								u4_format_version;						/* フォーマットバージョン */
	S4								s4_central_timestamp_;					/* ロジック用タイムスタンプ */
	U2								u2_size_lane_topology_blocks;			/* レーントポロジブロックリストの有効要素数 */
	U1								u1_size_lane_geometry_blocks;			/* レーンジオメトリブロックリストの有効要素数 */
	U1								u1_size_feature_blocks;					/* 地物ブロックリストの有効要素数 */
	U1								u1_handness_of_traffic;					/* 通行区分 */
	U1								u1_matching_count;						/* 別道路マッチングカウンタ */
	U1								u1_sis_state;							/* SIS状態 */
	U1								u1_navi_state;							/* ナビ状態 */
	U1								u1_center_state;						/* センター状態 */
} ST_RawSpatialInformationData;												/* SIS-ECUからの空間情報 */

typedef struct {
	ST_LaneCenterPoint				ast_lane_centers[CU4_SIZE_LANE_CENTERS_G];	/* レーン中心点の形状と属性 */
	ST_LaneMarker					ast_lane_markers[CU4_SIZE_LANE_MARKERS_G];	/* 区画線の形状 */
	ST_LaneMarkerPoint				ast_lane_marker_points[CU4_SIZE_LANE_MARKER_POINTS_G];	/* 区画線点列 */
	ST_BoundaryGeometry				ast_boundary_geometries[CU4_SIZE_BOUNDARY_GEOMETRIES_G];	/* 道路境界線形状 */
	ST_RoadSignGeometry				ast_road_signs[CU4_SIZE_ROAD_SIGNS_G];	/* 標識の形状 */
	ST_TrafficLightGeometry			ast_traffic_lights[CU4_SIZE_TRAFFIC_LIGHTS_G];	/* 信号機の形状 */
	ST_RoadMarkingGeometry			ast_road_markings[CU4_SIZE_ROAD_MARKINGS_G];	/* 路面標示の形状 */
	U4								u4_size_lane_centers;					/* レーン中心点の形状と属性の有効要素数 */
