typedef struct {
	U1								u1_CTL_OUT_OF_PATH_FOLLOWING_FUNCTION_ERROR;	/* 軌道追従異常（システム限界） */
	U1								u1_CTL_OUT_OF_SAFETY_FUNCTION_ERROR;	/* セーフティメカニズム異常
（異常時に切り替える先のモードが動かない） */
	U1								u1_CTL_OUT_OF_DRIVER_STEERING_JUDGMENT_FROM_EPS;	/* ステアリング操作によるEPSシステム終了判定結果 */
	U1								u1_CTL_OUT_OF_VEHICLE_ROLL_BACK_STATE;	/* ロールバック状態（車両後退） */
