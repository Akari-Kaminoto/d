	U1								u1_CTL_OUT_OF_MAP_SLOPE_OUT_OF_RANGE;	/* ブレーキ動作範囲外の勾配 */
	U1								u1_CTL_OUT_OF_STEER_ANGLE_OUT_OF_RANGE;	/* ステアリング操舵角規定範囲外 */
	U1								u1_CTL_OUT_OF_VX_COMPARISON_ERROR;		/* 車体速比較異常 */
	U1								u1_CTL_OUT_OF_GX_COMPARISON_ERROR;		/* 車体前後G比較異常 */
	U1								u1_CTL_OUT_OF_GY_COMPARISON_ERROR;		/* 車体横G比較異常 */
