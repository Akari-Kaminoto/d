	U4								au4_branch_right_pre[CU4_SIZE_BRANCH_RIGHT_PRE_LL];	/* 右斜め後のリンクへのインデックス */
	U4								au4_branch_left_pre[CU4_SIZE_BRANCH_LEFT_PRE_LL];	/* 左斜め後のリンクへのインデックス */
	U1								u1_size_next;							/* 前方のリンクへのインデックスの有効要素数 */
	U1								u1_size_previous;						/* 後方のリンクへのインデックスの有効要素数 */
	U1								u1_size_right;							/* 右のリンクへのインデックスの有効要素数 */
	U1								u1_size_left;							/* 左のリンクへのインデックスの有効要素数 */
	U1								u1_size_branch_right;					/* 右斜め前のリンクへのインデックスの有効要素数 */
	U1								u1_size_branch_left;					/* 左斜め前のリンクへのインデックスの有効要素数 */
	U1								u1_size_branch_right_pre;				/* 右斜め後のリンクへのインデックスの有効要素数 */
