	FL								fl_fl_g_cln_cmn_yaw;					/* 走路認識用ヨーレート */
	U1								u1_fg_g_cln_rcg_stt;					/* 認識センサ状態 */
	U1								u1_u1_g_cln_ln_change;					/* レーンチェンジフラグ */
	U1								u1_fg_g_cln_road_work_mode;				/* 工事区間モードフラグ */
	U1								u1_fg_g_cln_crossing;					/* 交差点判定フラグ */
	U1								u1_fg_g_cln_urban;						/* 市街地判定フラグ */
	U1								u1_fg_g_cln_highway;					/* 高速道路判定フラグ */
	U1								u1_fg_g_cln_slope;						/* 勾配判定フラグ */
	U1								u1_fg_g_cln_curve_entr;					/* カーブ入口判定フラグ */
	U1								u1_fg_g_cln_curve_exit;					/* カーブ出口判定フラグ */
	U1								u1_fg_g_cln_crs_line;					/* 交差線判定フラグ */
	U1								u1_u1_g_cln_cmn_ctr;					/* 認識カウンタ */
	U1								u1_fg_g_cln_sws_error_lft;				/* SWS用左側異常フラグ */
	U1								u1_fg_g_cln_sws_error_rgt;				/* SWS用右側異常フラグ */
	U1								u1_fg_g_cln_sws_road_error;				/* SWS路面異常フラグ */
	U1								u1_u1_g_cln_env_conf_lda;				/* LDA用環境自信度 */
	U1								u1_u1_g_cln_env_conf_ltc;				/* LTC用環境自信度 */
	U1								u1_u1_g_cln_env_conf_acc;				/* ACC用環境自信度 */
} ST_data_clane_cmn;														/* クレーン共通データ */

