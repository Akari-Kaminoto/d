	S1								s1_tollgate_flag;						/* 料金所区間フラグ */
	S1								s1_no_lane_flag;						/* 無車線フラグ */
	S1								s1_bus_stop_flag;						/* バス停フラグ */
	S1								s1_slower_lane_flag;					/* 登坂車線フラグ */
	S1								s1_turn_over_lane_flag;					/* ゆずり車線フラグ */
	S1								s1_branch_merge_devision;				/* 分合流区分 */
	S1								s1_accel_decel_devision;				/* 加速・減速車線区分 */
	S1								s1_is_permit_right_lc;					/* 右車線変更可否 */
	S1								s1_is_permit_left_lc;					/* 左車線変更可否 */
	S1								s1_is_covered;							/* 覆い物フラグ */
	U1								u1_road_type;							/* 道路種別 */
	S1								s1_road_link_type;						/* 道路リンク種別 */
	U1								u1_spatial_info_boundary;				/* 高精度地図境界情報 */
} ST_RamMoni_LinkAttribute;													/* リンク属性(RAMモニタ用) */

typedef struct RamMoni_LaneLink {
	ST_LaneLinkId					st_link_id;								/* レーンリンクID */
