	DB								db_target_speed_for_lk;					/* 元レーン制限速度 */
	DB								db_target_speed_for_lc;					/* 目標レーン制限速度 */
	DB								db_branch_progress_ratio;				/* 分岐路終端までの進行比率 */
	S4								s4_original_lane_id;					/* 元レーンのID */
	S4								s4_target_lane_id;						/* 目標レーンのID */
