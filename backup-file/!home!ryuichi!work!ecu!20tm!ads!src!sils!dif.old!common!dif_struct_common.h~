	ST_ENUPosition					st_bottom_right;						/* 信号機の底辺位置（右） */
	ST_ENUPosition					st_bottom_left;							/* 信号機の底辺位置（左） */
	ST_LaneLinkId					ast_link_ids[10];						/* レーンリンクIDリスト */
	U8								u8_traffic_light_id;					/* 信号機ID */
	FL								fl_height;								/* 高さ */
	FL								fl_depth;								/* 奥行き */
	U1								u1_triffic_signal_type;					/* 信号機種別 */
} ST_RamMoni_TrafficLightGeometry;											/* 信号機の形状(RAMモニタ用) */

typedef struct RamMoni_RoadMarkingGeometry {
	ST_ENUPosition					ast_positions[10];						/* 存在領域を囲うポリゴンの座標 */
	ST_LaneLinkId					ast_link_ids[10];						/* レーンリンクIDリスト */
	ST_RoadMarkingAttribute			st_attribute;							/* 路面標示属性 */
	U8								u8_road_marking_id;						/* 路面標示ID */
} ST_RamMoni_RoadMarkingGeometry;											/* 路面標示の形状(RAMモニタ用) */

