	S4								s4_plan_type;							/* プラン情報 */
	S4								s4_lc_direction;						/* LC方向 */
	S4								s4_lc_notice;							/* 分合流事前通知 */
	S4								s4_lc_trigger_acceptance_reason;		/* 手動トリガ受付可否・理由 */
	S4								s4_ready_inhibit_reason;				/* READY抑制理由 */
	S4								s4_suspend_reason;						/* サスペンド理由 */
	S4								s4_state_trans_req_sb;					/* SB遷移要求 */
	S4								s4_state_trans_req_td_fin;				/* TD終了要求 */
	U1								u1_navi_route_info;						/* ナビルート情報 */
	U1								u1_dss_trans_req;						/* DSSへの制御引継ぎ要求 */
} ST_RamMoni_LanePlan;														/* レーンプラン出力(RAMモニタ用) */

