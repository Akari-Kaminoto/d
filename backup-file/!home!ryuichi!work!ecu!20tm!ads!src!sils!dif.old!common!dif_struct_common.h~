typedef struct {
	FL								fl_out_o_can_ADSPD_R;					/* ADS制御車速(冗長) */
	FL								fl_out_o_can_ADSRDECG;					/* ADS加減速要求Ｇ(CL) */
	FL								fl_out_o_can_ADTQCP_R;					/* アシスト補正ゲイン（電子ASは1固定） */
	FL								fl_out_o_can_STSAV_R;					/* 直進舵角 */
	FL								fl_out_o_can_TGTSAV_R;					/* 目標操舵角指令値 */
	FL								fl_out_o_can_TGTSVV_R;					/* 目標操舵角速度指令値 */
	FL								fl_out_o_can_TGTSAV;					/* 目標操舵角指令値 */
	FL								fl_out_o_can_TGTSVV;					/* 目標操舵角速度指令値 */
	FL								fl_out_o_can_ADSPD;						/* ADS制御車速 */
	FL								fl_out_o_can_ADTQCP;					/* アシスト補正ゲイン(電子ASは1固定) */
	FL								fl_out_o_can_STSAV;						/* 直進舵角 */
	FL								fl_out_o_can_ADSDECG;					/* ADS加減速要求Ｇ */
	FL								fl_out_o_can_ADSRQM;					/* ADS要求モーメント */
	FL								fl_out_o_can_TGBKRFR;					/* FR輪目標制動力比 */
	FL								fl_out_o_can_TGBKRFL;					/* FL輪目標制動力比 */
	FL								fl_out_o_can_TGBKRRR;					/* RR輪目標制動力比 */
	FL								fl_out_o_can_TGBKRRL;					/* RL輪目標制動力比 */
	FL								fl_out_o_can_ADSFDRV;					/* ADSﾄｰﾀﾙ要求駆動力 */
	FL								fl_out_o_can_ADSFSEL;					/* ADSセレクト要求駆動力 */
	FL								fl_out_o_can_ADSFLIM;					/* ADS駆動力制限要求 */
	U1								u1_OUT_OF_CAN_ADSDCP_R;					/* ADS　ドライバ協調中フラグ（電子ASは0固定） */
	U1								u1_OUT_OF_CAN_ADSEF_R;					/* ADSシステム有効フラグ */
	U1								u1_OUT_OF_CAN_ADSHF_R;					/* ADS　HandsFree中フラグ（電子ASは0固定） */
	U1								u1_OUT_OF_CAN_ADSMLF_R;					/* ADSダイアグ異常状態 */
	U1								u1_OUT_OF_CAN_ADSPEF_R;					/* ADS制御車速有効フラグ(冗長) */
	U1								u1_OUT_OF_CAN_CINRTF_R;					/* 協調時積分リセットフラグ（電子ASは0固定） */
	U1								u1_OUT_OF_CAN_CINSPF_R;					/* 協調時積分停止フラグ(冗長) */
	U1								u1_OUT_OF_CAN_EPLMCT_R;					/* EPS制限時制御中フラグ（電子ASは0固定） */
	U1								u1_OUT_OF_CAN_LRDIFF_R;					/* 左右差付与フラグ（電子ASは0固定） */
	U1								u1_OUT_OF_CAN_SINRTF_R;					/* 直進舵角更新時積分リセットフラグ（電子ASは0固定） */
	U1								u1_OUT_OF_CAN_SINSPF_R;					/* 直進舵角更新時積分停止（電子ASは0固定） */
	U1								u1_OUT_OF_CAN_STRCSW_R;					/* 操舵制御C1→CL切替フラグ（電子ASは0固定） */
	U1								u1_OUT_OF_CAN_STSAEF_R;					/* 直進舵角有効フラグ */
	U1								u1_OUT_OM_CAN_ADCHM_R;					/* ADS制御特性モード（電子ASは0固定） */
	U1								u1_OUT_OM_CAN_ADSTB_R;					/* ADS制御状態(CL) */
	U1								u1_OUT_OM_CAN_ADSTS_R;					/* EPS用　ADS制御状態 */
	U1								u1_OUT_OM_CAN_FODEM_R;					/* フェードアウト処理要求 */
	U1								u1_OUT_OF_CAN_ADSBHOK;					/* ブレーキホールド許可フラグ */
	U1								u1_OUT_OM_CAN_ADSRQBH;					/* ブレーキホールド実行要求 */
	U1								u1_OUT_OM_CAN_ADSTB;					/* ECB用　ADS制御状態 */
	U1								u1_OUT_OF_CAN_ADSEF;					/* ADSシステム有効フラグ */
	U1								u1_OUT_OF_CAN_ADSMLF;					/* ADSダイアグ異常状態 */
	U1								u1_OUT_OF_CAN_EPLMCT;					/* EPS制限時制御中フラグ(電子ASは0固定) */
	U1								u1_OUT_OF_CAN_STRCSW;					/* 操舵制御C1→CL切替フラグ（電子ASは0固定値を送る） */
	U1								u1_OUT_OM_CAN_ADSTS;					/* EPS用　ADS制御状態 */
	U1								u1_OUT_OF_CAN_ADSDCP;					/* ADS　ドライバ協調中フラグ(電子ASは0固定) */
	U1								u1_OUT_OF_CAN_ADSHF;					/* ADS　HandsFree中フラグ(電子ASは0固定) */
	U1								u1_OUT_OF_CAN_ADSPEF;					/* ADS制御車速有効フラグ */
	U1								u1_OUT_OF_CAN_CINRTF;					/* 協調時積分リセットフラグ(電子ASは0固定) */
	U1								u1_OUT_OF_CAN_CINSPF;					/* 協調時積分停止フラグ */
	U1								u1_OUT_OF_CAN_LRDIFF;					/* 左右差付与フラグ(電子ASは0固定) */
	U1								u1_OUT_OM_CAN_ADCHM;					/* ADS制御特性モード(電子ASは0固定) */
	U1								u1_OUT_OM_CAN_FODEM;					/* フェードアウト処理要求(電子ASはADS制御状態の反対） */
	U1								u1_OUT_OM_CAN_STPRQADS;					/* LDH停止要求（EPSは電子AS処理しない） */
	U1								u1_OUT_OF_CAN_SINRTF;					/* 直進舵角更新時積分リセットフラグ(電子ASは0固定) */
	U1								u1_OUT_OF_CAN_SINSPF;					/* 直進舵角更新時積分停止(電子ASは0固定) */
	U1								u1_OUT_OF_CAN_STSAEF;					/* 直進舵角有効フラグ(電子ASは1固定) */
	U1								u1_OUT_OF_CAN_ADSBRKOK;					/* ブレーキ許可フラグ */
	U1								u1_OUT_OF_CAN_ADSNOCHG;					/* ADS充電制御禁止要求 */
	U1								u1_OUT_OF_CAN_ADSMRF;					/* ADS要求モーメント有効 */
	U1								u1_OUT_OF_CAN_ADSBRKCN;					/* ADS-BRK協調制御中フラグ */
	U1								u1_OUT_OF_CAN_ADSRQSFT;					/* ADS要求シフトレンジ */
	U1								u1_OUT_OM_CAN_ADSTPF;					/* EHV用 ADS制御状態(駆動力) */
	U1								u1_OUT_OF_CAN_ADSAVSTR;					/* ADS AVS起動フラグ */
} ST_CTRL_CAN_OUT;															/* CAN出力 */

typedef struct {
	FL								fl_ctl_out_o_lateralAccLimitValue;		/* 許容横加速度 */
	FL								fl_ctl_out_o_longitudinalAccLimitValue;	/* 許容前後加速度 */
	FL								fl_ctl_out_o_relativeLongitudinalAcceleration;	/* 対地加速度 */
	FL								fl_ctl_out_o_attgtBrk;					/* ブレーキ制御用目標加速度 */
	FL								fl_ctl_out_o_estRoadSlope;				/* 推定路面勾配 */
	FL								fl_ctl_out_o_ctlPinionAngleWithSTRZERO;	/* 直進舵角補正後制御用ピニオン角 */
	FL								fl_ctl_out_o_strAngZero;				/* 直進舵角記憶値 */
	FL								fl_ctl_out_o_ReducedSpeed;				/* 抑制速度 */
	FL								fl_ctl_out_o_driverSteeringOperationTorque;	/* ドライバ操舵トルク値 */
	FL								fl_ctl_out_o_epsPinionAngularVelocity;	/* 制御用EPSピニオン角速度 */
	FL								fl_ctl_out_o_can_hv_accp_for_pln;		/* PLN用アクセル開度 */
	FL								fl_ctl_out_o_vclSlipAngleLinear;		/* 線形域用推定車体スリップ角 */
	FL								fl_ctl_out_o_vclSlipAngle;				/* 推定車体スリップ角 */
	S2								s2_ctl_out_o_fdrvtotal;					/* ADSトータル要求駆動力 */
	S2								s2_ctl_out_o_attgt_ads;					/* ADSｸﾙｰｽﾞ目標加速度 */
	U1								u1_CTL_OUT_OM_ACCEL_MANUAL_INPUT;		/* アクセルペダルへのドライバ入力状態 */
	U1								u1_CTL_OUT_OM_STEER_MANUAL_INPUT;		/* ステアリングホイールへのドライバ入力状態 */
	U1								u1_CTL_OUT_OM_BRAKE_MANUAL_INPUT;		/* ブレーキペダルへのドライバー入力状態 */
	U1								u1_CTL_OUT_OM_PATH_REPLAN_REQUEST_FOR_STEER_COOP;	/* 操舵による経路リプラン要求 */
	U1								u1_CTL_OUT_OM_PATH_REPLAN_REQUEST;		/* 経路リプラン要求 */
	U1								u1_CTL_OUT_OF_PATH_REPLAN_PROHIBIT_REQUEST;	/* パスリプラン禁止要求 */
	U1								u1_CTL_OUT_OF_SPEED_REPLAN_REQUEST;		/* スピードリプラン要求 */
	U1								u1_CTL_OUT_OM_EHV_ADSTPF_FLAG;			/* EHV用(駆動力) ADS制御指令フラグ */
	U1								u1_CTL_OUT_OF_ACC_LIMIT_REQUEST;		/* 許容加速度要求フラグ */
	U1								u1_CTL_OUT_SPEED_REDUCE_REQUEST;		/* 速度抑制要求 */
	U1								u1_CTL_OUT_OM_AUTO_SPEED_CONTROL;		/* 前後制御状態 */
	U1								u1_CTL_OUT_OM_AUTO_STEER_CONTROL;		/* 横制御状態 */
	U1								u1_CTL_OUT_OF_RELATIVE_LONGITUDINAL_ACCELERATION_VALID;	/* 対地加速度有効 */
	U1								u1_CTL_OUT_OM_REQ_STT_BRK;				/* ブレーキ制御要求状態 */
	U1								u1_CTL_OUT_OM_PINION_ANGLE_STATE;		/* 制御用ピニオン角状態 */
	U1								u1_CTL_OUT_OF_STRANGZERO_MEMORY;		/* 直進舵角記憶実施フラグ */
	U1								u1_CTL_OUT_OF_DRIVER_STEERING_OPERATION_DETECTION_VALID;	/* ドライバ操舵判定結果有効フラグ */
	U1								u1_CTL_OUT_OF_DRIVER_STEERING_OPERATION_TORQUE_VALID;	/* ドライバ操舵トルク値有効フラグ */
	U1								u1_CTL_OUT_OF_DRIVER_STEERING_OPERATION_DETECTION;	/* ドライバ操舵判定結果 */
	U1								u1_CTL_OUT_OM_OVER_LATERAL_ACCELERATION;	/* 横G超過判定結果 */
	U1								u1_CTL_OUT_OF_EPS_PINION_ANGULAR_VELOCITY_VALID;	/* 制御用EPSピニオン角速度有効フラグ */
	U1								u1_CTL_OUT_OF_CAN_B_PDLF_FOR_PLN;		/* PLN用アクセル開度信頼性情報 */
	U1								u1_CTL_OUT_OF_VCL_SLIP_ANGLE_LINEAR_VALID;	/* 線形域用推定車体スリップ角有効フラグ */
	U1								u1_CTL_OUT_OF_VCL_SLIP_ANGLE_VALID;		/* 推定車体スリップ角有効フラグ */
} ST_CTRL_ADS_OUT;															/* ADS内出力 */

typedef struct {
	S4								s4_lc_type;								/* LC種別 */
	S4								s4_new_speed;							/* LC提案速度 */
	U1								u1_approval_result;						/* 車線変更承認状況 */
	U1								u1_new_speed_unit;						/* LC提案速度単位切替 */
} ST_HsiPlannerData;														/* HSIプランナデータ */

typedef struct {
	U1								u1_lta_approval_result;					/* ACC+LTA切替提案承認状況 */
	U1								au1_hsi_td_reason[CU4_SIZE_HSI_TD_REASON_HSD];	/* ドライバ不調による手動運転切替理由 */
	U1								u1_size_hsi_td_reason;					/* ドライバ不調による手動運転切替理由の有効要素数 */
	U1								u1_ads_approval_result;					/* ADS切替提案承認状況 */
} ST_HsiSsmData;															/* HSISSMデータ */

typedef struct {
	FL								fl_str_vib_ontime;						/* モータON時間 */
	FL								fl_str_vib_offtime;						/* モータOFF時間 */
	FL								fl_str_vib_time;						/* 制御時間 */
	U1								u1_str_vib_req;							/* 振動要求 */
	U1								u1_str_vib_str;							/* ステアリング振動強度 */
} ST_HsiFcmData;															/* HSIFCMデータ */

typedef struct {
	U4								u4_data_receive_timestamp;				/* データ取得タイムスタンプ */
	U4								u4_data_reference_timestamp;			/* データ基準タイムスタンプ */
	FL								fl_speed_std;							/* 標準車速 */
	FL								fl_yawrate_std;							/* 標準ヨーレート */
	FL								fl_gx_std;								/* 標準縦加速度 */
	FL								fl_gx_sensor_std;						/* 標準センサ縦加速度 */
	FL								fl_gy_sensor_std;						/* 標準センサ横加速度 */
	FL								fl_speed_opt;							/* オプション車速 */
	FL								fl_yawrate_opt;							/* オプションヨーレート */
	FL								fl_gx_sensor_opt;						/* オプションセンサ縦加速度 */
	FL								fl_gy_sensor_opt;						/* オプションセンサ横加速度 */
	U1								u1_bkflg_speed_std;						/* 標準車速バックアップ状態 */
	U1								u1_bkflg_yawrate_std;					/* 標準ヨーレートバックアップ状態 */
	U1								u1_bkflg_gx_std;						/* 標準縦加速度バックアップ状態 */
	U1								u1_bkflg_gx_sensor_std;					/* 標準センサ縦加速度バックアップ状態 */
	U1								u1_bkflg_gy_sensor_std;					/* 標準センサ横加速度バックアップ状態 */
	U1								u1_bkflg_speed_opt;						/* オプション車速バックアップ状態 */
	U1								u1_bkflg_yawrate_opt;					/* オプションヨーレートバックアップ状態 */
	U1								u1_bkflg_gx_sensor_opt;					/* オプションセンサ縦加速度バックアップ状態 */
	U1								u1_bkflg_gy_sensor_opt;					/* オプションセンサ横加速度バックアップ状態 */
} ST_CarState;																/* 車両状態 */

typedef struct {
	ST_CarState						st_newest_pose;							/* 最新車両状態 */
} ST_CarData;																/* 車両データ */

typedef struct {
	ST_CarState						st_pose;								/* 車両情報 */
	U1								u1_dbklab;								/* D席バックルSW信号 */
} ST_ABG;																	/* ABGデータ */

typedef struct {
	ST_CarState						st_pose;								/* 車両情報 */
	U1								u1_b_lsus12;							/* コンフォートランプ */
	U1								u1_b_lsus6;								/* LOランプ */
	U1								u1_b_lsus4;								/* NORMALランプ */
	U1								u1_b_lsus2;								/* HIランプ */
	U1								u1_b_lsus14;							/* 車高制御方向 */
	U1								u1_b_lsus10;							/* 乗降モードランプ */
	U1								u1_b_lsus8;								/* OFFランプ */
	U1								u1_asccus;								/* カスタマイズ状態 */
	U1								u1_ascstop2;							/* 乗降制御禁止状態 */
	U1								u1_ascstop1;							/* 車高制御禁止状態 */
	U1								u1_asccusf;								/* カスタマイズスイッチ表示 */
	U1								u1_ascstpf2;							/* 乗降制御禁止スイッチ表示 */
	U1								u1_ascstpf1;							/* 車高制御禁止スイッチ表示 */
	U1								u1_hrlf;								/* RL輪車高センサ異常 */
	U1								u1_hrrf;								/* RR輪車高センサ異常 */
	U1								u1_hflf;								/* FL輪車高センサ異常 */
	U1								u1_hfrf;								/* FR輪車高センサ異常 */
	U1								u1_ascminf;								/* マルチインフォ要求 */
	U1								u1_b_lsus16;							/* 荷物モードランプ */
} ST_ASC;																	/* ASCデータ */

typedef struct {
	ST_CarState						st_pose;								/* 車両情報 */
	FL								fl_cvr1;								/* 曲率半径(400m先) */
	FL								fl_mldt1;								/* 分岐／合流開始までの距離 */
	FL								fl_mldt2;								/* 分岐／合流終了までの距離 */
	FL								fl_lnaddt1;								/* 車線増減開始までの距離 */
	FL								fl_lnaddt2;								/* 車線増減終了までの距離 */
	U1								u1_confid01;							/* 自信度ID 1 */
	U1								u1_conf;								/* 自信度判定 */
	U1								u1_lyt;									/* リンク種別判定信号 */
	U1								u1_drvw;								/* 自専道判定信号 */
	U1								u1_branch;								/* 高速分岐点信号 */
	U1								u1_tunl;								/* 道路構造物：トンネル */
	U1								u1_leaveout;							/* 退出レーン走行信号 */
	U1								u1_mrglane;								/* 合流レーン走行信号 */
	U1								u1_senspfm;								/* センサ状態 */
	U1								u1_sensfail;							/* センサ異常 */
	U1								u1_confid11;							/* 自信度ID 11 */
	U1								u1_confmmc;								/* マッチング確度 */
	U1								u1_disterr;								/* 前後方向精度 */
	U1								u1_conflane;							/* 横(レーン)方向精度 */
	U1								u1_confdir;								/* 将来位置精度 */
	U1								u1_a_lang;								/* 言語設定 */
	U1								u1_lvmain;								/* 本線退出信号 */
	U1								u1_mlid;								/* 分岐／合流のインデックス */
	U1								u1_mldir;								/* 分岐／合流の方向 */
	U1								u1_mlct;								/* 分岐／合流の接続種別 */
	U1								u1_mltdir;								/* 分岐／合流の進行方向 */
	U1								u1_mllnum;								/* 分岐／合流レーン数 */
	U1								u1_mlptn;								/* 分岐／合流形状パターン */
	U1								u1_lnadid;								/* 車線増減のインデックス */
	U1								u1_lnaddir;								/* 車線増減の方向 */
	U1								u1_lnadptn;								/* 車線増減のパターン */
	U1								u1_lnadojln;							/* 車線増減対象レーン */
	U1								u1_lnadnum;								/* 車線増減数 */
} ST_AVN;																	/* AVNデータ */

typedef struct {
	ST_CarState						st_pose;								/* 車両情報 */
	U1								u1_c_code1;								/* 国番号1桁目 */
	U1								u1_tpm_flpr;							/* 空気圧モニタ FL輪(ID1)の圧力値 */
