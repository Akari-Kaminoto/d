	U4								au4_traffic_lights[CU4_SIZE_TRAFFIC_LIGHTS_LG];	/* レーンリンクに存在する信号へのインデックス */
	U4								au4_road_markings[CU4_SIZE_ROAD_MARKINGS_LG];	/* レーンリンクに存在する路面標示へのインデックス */
	FL								fl_distance_from_start;					/* レーンリンク始端から地図切り出し範囲までの距離 */
	U2								u2_size_centers;						/* レーンリンクの中心座標点へのインデックスの有効要素数 */
	U2								u2_size_traffic_lights;					/* レーンリンクに存在する信号へのインデックスの有効要素数 */
	U1								u1_size_lefts;							/* レーンリンクの左区画線へのインデックスの有効要素数 */
	U1								u1_size_rights;							/* レーンリンクの右区画線へのインデックスの有効要素数 */
	U1								u1_size_straddles;						/* レーンリンクを跨ぐ区画線へのインデックスの有効要素数 */
	U1								u1_size_left_boundary_geometries;		/* レーンリンクの左道路境界線へのインデックスの有効要素数 */
	U1								u1_size_right_boundary_geometries;		/* レーンリンクの右道路境界線へのインデックスの有効要素数 */
	U1								u1_size_road_signs;						/* レーンリンクに存在する標識へのインデックスの有効要素数 */
	U1								u1_size_road_markings;					/* レーンリンクに存在する路面標示へのインデックスの有効要素数 */
