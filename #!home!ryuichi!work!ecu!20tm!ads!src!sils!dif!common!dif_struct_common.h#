	S4								s4_lc_dest_lane_id;						/* ドライバ由来LC先レーンID */
	U1								u1_abort_lc;							/* ドライバによる中止要求 */
	U1								u1_trig_reject_reason;					/* ドライバトリガ受付不可理由 */
} ST_DriverLcDemand;														/* ドライバLC要求 */

typedef struct {
	ST_SystemLcDemand				st_system_lc_demand;					/* システムLC要求 */
	ST_DriverLcDemand				st_driver_lc_demand;					/* ドライバLC要求 */
	S4								s4_lc_start_lane_id;					/* LC元レーンID */
} ST_LaneChangeCommand;														/* レーンチェンジ要求 */

typedef struct {
	S4								s4_lc_dest_lane_id;						/* ドライバ由来LC先レーンID */
	S4								s4_lc_direction;						/* ドライバ由来LC方向 */
	S4								s4_lc_aggressive;						/* ドライバ由来LC割込モード */
	S4								s4_trig_reject_reason;					/* ドライバトリガ受付不可理由 */
	U1								u1_abort_lc;							/* ドライバによる中止要求 */
} ST_DriverLcDemandRAW;														/* ドライバトリガLC要求解釈結果 */

typedef struct {
