	DB								db_SetVAccelLimitGxH;					/* SetV走行最大Ｇ上限値（マップの高速側） */
	DB								db_SetVAccelLimitGxLV;					/* SetV走行最大Ｇ上限値（低速側）対応車速 */
	DB								db_SetVAccelLimitGxHV;					/* SetV走行最大Ｇ上限値（高速側）対応車速 */
	DB								db_RapidBrakeReleaseTime;				/* ブレーキ早抜き時定数 */
	DB								db_AccelReleaseTime;					/* アクセルリリース時定数 */
	DB								db_AccelReleaseMinimumJerk;				/* アクセルリリース最小Jerk */
	DB								db_AccelReleaseTargetA;					/* 減速側目標G係数 */
	DB								db_SetVDecelLimitGx;					/* 減速側許容最大減速度 */
	DB								db_BrakeApplyTime;						/* ブレーキ踏み増し時定数 */
	DB								db_BrakeApplyMinimumJerk;				/* ブレーキ踏み増し最小Jerk */
	DB								db_DecelLimitGxEnactedFusion;			/* 追従走行許容減速度上限(Fusion) */
	DB								db_DecelLimitGxFailureFusion;			/* 追従走行許容減速度上限(非Fusion) */
	DB								db_BrakeReleaseMinimumJerk;				/* ブレーキ抜き最小Jerk */
	DB								db_BrakeReleaseTime;					/* ブレーキ抜き時定数 */
	DB								db_FollowControlStartGSenstivity;		/* 対先行車減速を開始する減速度感度閾値 */
	DB								db_FollowControlKeepGThresh;			/* 遅い先行車に対しG増加を禁止する閾値 */
	DB								db_AccelLimitGxL;						/* 追従走行許容G上限値（低速側） */
	DB								db_AccelLimitGxH;						/* 追従走行許容G上限値（高速側） */
	DB								db_AccelLimitGxLV;						/* 追従走行許容G上限値（低速側）対応車速 */
	DB								db_AccelLimitGxHV;						/* 追従走行許容G上限値（高速側）対応車速 */
