	U1								u1_plnstsin_deslc_direction;			/* DES-LC方向 */
	U1								u1_plnstsin_ess_sts;					/* ESS作動状態 */
	U1								u1_plnstsin_hazard_sw;					/* ハザードSW状態 */
	U1								u1_plnstsin_brake_sts;					/* ブレーキ操作状態 */
