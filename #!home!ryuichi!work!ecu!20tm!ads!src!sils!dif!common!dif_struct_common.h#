	U1								u1_str_vib_str;							/* ステアリング振動強度 */
} ST_HsiFcmData;															/* HSIFCMデータ */

typedef struct {
	FL								fl_raw_yaw_rate;						/* ヨーレート(学習前) */
	FL								fl_yaw_rate;							/* ヨーレート(学習後) */
	FL								fl_velocity;							/* 推定車速 */
	FL								fl_gx;									/* 加速度（進行方向） */
	FL								fl_gy;									/* 加速度（左右方向） */
	U1								u1_yaw_rate_fail;						/* ヨーレートセンサ故障 */
	U1								u1_yaw_rate_af;							/* ヨーレート学習完了 */
	U1								u1_raw_yaw_rate_reliability;			/* ヨーレート精度(学習前) */
	U1								u1_yaw_rate_reliability;				/* ヨーレート精度(学習後) */
	U1								u1_velocity_af;							/* 推定車速学習完了 */
	U1								u1_velocity_reliability;				/* 推定車速精度 */
	U1								u1_g_status;							/* 加速度センサ故障 */
	U1								u1_fail;								/* 故障検出フラグ */
} ST_SISSensorData;															/* SISセンサデータ */

typedef struct {
	FL								fl_x;									/* Ｘ位置 */
	FL								fl_y;									/* Ｙ位置 */
} ST_FRDPosition2D;															/* FRD物標相対位置 */

typedef struct {
	FL								afl_polar_coordinates_ground[CU4_SIZE_POLAR_COORDINATES_GROUND_PMI];	/* 極座標 地面 */
	FL								afl_polar_coordinates_obstacle[CU4_SIZE_POLAR_COORDINATES_OBSTACLE_PMI];	/* 極座標 障害物 */
	FL								fl_polar_ground_dist;					/* 極座標 路面距離 */
	U2								u2_size_polar_coordinates_ground;		/* 極座標 地面の有効要素数 */
	U2								u2_size_polar_coordinates_obstacle;		/* 極座標 障害物の有効要素数 */
	U1								u1_synchronize_counter;					/* 同期カウンタ */
	U1								au1_polar_coordinates_obstacle_smoke_judge[CU4_SIZE_POLAR_COORDINATES_OBSTACLE_SMOKE_JUDGE_PMI];	/* 極座標 障害物煙フラグ */
	U1								u1_size_polar_coordinates_obstacle_smoke_judge;	/* 極座標 障害物煙フラグの有効要素数 */
} ST_PolarMapInfo;															/* 極座標情報 */

typedef struct {
	FL								fl_thtemp;								/* サーミスタ情報 */
	U1								u1_grphldl;								/* 手放し判定結果(左) */
	U1								u1_grphldr;								/* 手放し判定結果(右) */
	U1								u1_tmpabnl;								/* センサ仮異常(左) */
	U1								u1_tmpabnr;								/* センサ仮異常(右) */
	U1								u1_grpfail1;							/* グリップ異常1 */
	U1								u1_grpfail0;							/* グリップ異常0 */
	U1								u1_tmpmfix;								/* センサ中間値固着仮異常有無表示 */
	U1								u1_mterr;								/* モータ異常 */
	U1								u1_herr;								/* ヒータ異常 */
	U1								u1_nsabnl;								/* ノイズ異常 */
	U1								u1_vfpext;								/* 振動機能あり */
	U1								u1_gfpext;								/* グリップ検知機能あり */
	U1								u1_thmost;								/* サーモスタット状態 */
	U1								u1_heatst;								/* ヒータ作動状態 */
	U1								u1_grpexl;								/* 相対値法有効時間判定(左) */
	U1								u1_gfalll;								/* 微分立下り(左) */
	U1								u1_grisenl;								/* 微分立上り通常(左) */
	U1								u1_grisedl;								/* 微分立上り遅延(左) */
	U1								u1_grpexr;								/* 相対値法有効時間判定(右) */
	U1								u1_gfallr;								/* 微分立下り(右) */
	U1								u1_grisenr;								/* 微分立上り通常(右) */
	U1								u1_grisedr;								/* 微分立上り遅延(右) */
	U1								u1_tlinlost;							/* LIN通信仮途絶フラグ */
	U1								u1_linlost;								/* LIN通信途絶フラグ */
} ST_FCMTouchSensor;														/* タッチセンサ状態読み出し */

typedef struct {
	FL								fl_x;									/* 加速度X */
	FL								fl_y;									/* 加速度Y */
} ST_Acceleration2D;														/* 2D加速度構造体 */

typedef struct {
	FL								afl_ws_distortion_coefficients_[CU4_SIZE_WS_DISTORTION_COEFFICIENTS__DP];	/* W/S歪補正パラメータ */
	U1								u1_is_lens_distortion_;					/* レンズ歪補正実施フラグ */
	U1								u1_size_ws_distortion_coefficients_;	/* W/S歪補正パラメータの有効要素数 */
	U1								u1_is_ws_distortion_;					/* W/S歪補正実施フラグ */
} ST_DistortionParam;														/* 歪補正パラメータ */

typedef struct {
	U1								u1_camera_high_temperature_flag_;		/* カメラ高温フラグ */
	U1								u1_camera_low_temperature_flag_;		/* カメラ低温フラグ */
	U1								u1_camera_out_of_temperature_flag_;		/* カメラ動作保証温度外フラグ */
} ST_CameraTemperature;														/* カメラ温度フラグ */

typedef struct {
	U1								u1_camera_high_voltage_flag_;			/* カメラ高電圧フラグ */
	U1								u1_camera_low_voltage_flag_;			/* カメラ低電圧フラグ */
	U1								u1_camera_out_of_voltage_flag_;			/* カメラ動作保証電圧外フラグ */
} ST_CameraVoltage;															/* カメラ電圧フラグ */

typedef struct {
	U1								u1_camera_defect_flag_;					/* カメラハード故障フラグ */
	U1								u1_camera_failure_detect_flag_;			/* カメラ一時使用不可フラグ */
	U1								u1_back_channel_communication_blackout_flag_;	/* バックチャネル通信途絶フラグ */
	U1								u1_telephoto_back_channel_communication_blackout_flag_;	/* 望遠カメラ バックチャネル通信途絶フラグ */
} ST_CameraDiagnosis;														/* カメラハード故障 */

typedef struct {
	U1								u1_map_number_;							/* 露出制御Map No. */
} ST_CameraExposure;														/* 露出制御 */

typedef struct {
	U4								u4_size_buffer;							/* バッファの有効要素数 */
	U4								u4_height;								/* 高さ */
	U4								u4_width;								/* 幅 */
	U1								au1_buffer[CU4_SIZE_BUFFER_IMAGE__BYTE__];	/* バッファ */
} ST_Image__Byte__;															/* Byte画像構造体 */

typedef struct {
	U4								u4_size_buffer;							/* バッファの有効要素数 */
	U4								u4_height;								/* 高さ */
