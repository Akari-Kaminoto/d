/****************************************************************************/
/* ファイル名	:	IO_ether_mod_call_adx1s53e_4ms.c						*/
/*--------------------------------------------------------------------------*/
/* モジュール名	:	IO-Ether ADX1S53E 4msタスク処理							*/
/*--------------------------------------------------------------------------*/
/* 機能			:	4msタスクで駆動させるADX1S53E受信関数を登録し、			*/
/* 					外部提供関数としてスケジューラからコールさせる			*/
/****************************************************************************/

/****************************************************************************/
/*						ヘッダインクルード									*/
/****************************************************************************/
#include	<common/type.h>
#include	<dif/ioei004ms_adx1s53e_soc1/dif_funclv_dif_ioei004ms_adx1s53e_soc1.h>
#include	<dif/ioei004ms_adx1s53e_soc1/dif_funclv_mcucom_ioei004ms_adx1s53e_soc1.h>
#include	"./../io_include.h"

/****************************************************************************/
/*						関数宣言											*/
/****************************************************************************/


/****************************************************************************/
/*						マクロ定義											*/
/****************************************************************************/


/****************************************************************************/
/*						構造体型定義										*/
/****************************************************************************/


/****************************************************************************/
/*						定数定義											*/
/****************************************************************************/


/****************************************************************************/
/*						変数定義											*/
/****************************************************************************/


/****************************************************************************/
/* 関数名		: fn_io_module_call_ioei004ms_adx1s53e_soc1					*/
/*--------------------------------------------------------------------------*/
/* 機能			: 4ms更新(受信データ)関数									*/
/*--------------------------------------------------------------------------*/
/* 引数			: なし														*/
/* 戻り値		: なし														*/
/****************************************************************************/
VD fn_io_module_call_ioei004ms_adx1s53e_soc1(VD)
{
	/* IO-03-06 */
	fn_io_ether_receive_adx1s53e_4ms();

	if (u1_g_io_adx1s53e_output == CU1_DF_TRUE) {

		/* データ登録 */
		fn_dataout_dif_ioei004ms_adx1s53e_soc1();
		fn_dataout_mcucom_trns_ioei004ms_adx1s53e_soc1();

		/* フラグ初期化 */
		u1_g_io_adx1s53e_output = CU1_DF_FALSE;3

	}

	return;
}
